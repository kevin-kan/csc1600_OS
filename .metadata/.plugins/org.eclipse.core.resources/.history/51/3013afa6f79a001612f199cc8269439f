#include "usefulVariables.h"

void *dataFiller(void *sourceData){
	inputData *source = (sourceData*) sourceData;

	FILE* myfile = fopen(source-> sourceFileName, "r");

	for (int i = 0; i< source -> numObs; i++){
		fscanf(myfile, "%lf", source -> values++);
	}
	fclose(myfile);
	return 0;
}
