#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main(void) {
	printf("Project 4 Start"); 	// prints Project 4 Start

	int i = 64;
	while (i < 65536){
	//	FileTest();
		i = i*2;
	}

	return EXIT_SUCCESS;
}
